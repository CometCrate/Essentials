{
  "name": "server",
  "version": "1.0.0",
  "description": "CometCrate Essentials Server",
  "main": "./dist/server.js",
  "scripts": {
    "dev": "nodemon",
    "dev:exec": "node --inspect=5858 -r ts-node/register ./src/server.ts",
    "build": "rimraf dist && tsc"
  },
  "nodemonConfig": {
    "delay": 1000,
    "watch": [
      "src"
    ],
    "exec": "npm run dev:exec",
    "ext": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CometCrate/Essentials.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/CometCrate/Essentials/issues"
  },
  "homepage": "https://github.com/CometCrate/Essentials#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "compression": "^1.7.4",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "passport": "^0.4.0",
    "passport-oauth2": "^1.5.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/compression": "^1.0.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/passport": "^1.0.1",
    "@types/passport-oauth2": "^1.4.8",
    "@types/node": "^12.7.11",
    "nodemon": "^1.19.3",
    "rimraf": "^3.0.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  }
}
